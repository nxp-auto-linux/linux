		fsl,spi-cs-sck-delay = <100>;
		fsl,spi-sck-cs-delay = <100>;
		reg = <0>;

		dsa,member = < 0 0 >;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				label = "sw0-p0";
				phy-mode = "rgmii-id";
				ethernet = <&pfe_netif2>;
				reg = <0>;

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};

			enet_p1@1 {
				label = "enet_p1";
				phy-mode = "mii";
				phy-handle = <&pfe_mdio_a_phy6>;
				reg = <1>;
			};

			enet_p2@2 {
				label = "enet_p2";
				phy-mode = "mii";

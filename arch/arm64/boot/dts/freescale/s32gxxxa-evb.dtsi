			enet_p3@3 {
				label = "enet_p3";
				phy-mode = "rgmii-id";
				phy-handle = <&pfe_mdio_a_phy2>;
				reg = <3>;
			};

			enet_p4@4 {
				label = "enet_p4";
				phy-mode = "rgmii-id";
				phy-handle = <&pfe_mdio_a_phy3>;
				reg = <4>;
			};
		};
	};
};


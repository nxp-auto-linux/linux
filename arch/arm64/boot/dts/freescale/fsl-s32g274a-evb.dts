/dts-v1/;
#include "fsl-s32g274a.dtsi"
#include <dt-bindings/pinctrl/s32g274-pinctrl.h>
/ {
	model = "Freescale S32G274";
	compatible = "fsl,s32g274-simu", "fsl,s32g274",
				 "arm,vexpress,v2p-aarch64", "arm,vexpress";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	usbphynop: usbphynop {
		compatible = "usb-nop-xceiv";
		#phy-cells = <0>;
	};
};

&usbmisc {
	status = "okay";
};

&usbotg {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_usbotg>, <&pinctrl1_usbotg>;
	fsl,usbphy = <&usbphynop>;
	status = "okay";
};

&qspi {
	status = "okay";

	mx25uw51245g@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <200000000>;
		spi-tx-bus-width = <8>;
		spi-rx-bus-width = <8>;
		reg = <0>;
	};
};

&gmac0 {
	status = "okay";
	phy-mode = "rgmii";
	phy-handle = <&gmac0_mdio_c_phy4>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_gmac0_rgmii_c>;
};

&gmac0_mdio {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_gmac0_mdio_c>;
	/* KSZ9031RNX on S32G-VNP-PROC */
	gmac0_mdio_c_phy4: ethernet-phy@4 {
		compatible = "ethernet-phy-ieee802.3-c22";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <4>;
		rxdv-skew-ps = <480>;
		txen-skew-ps = <480>;
		rxd0-skew-ps = <480>;
		rxd1-skew-ps = <480>;
		rxd2-skew-ps = <480>;
		rxd3-skew-ps = <480>;
		txd0-skew-ps = <420>;
		txd1-skew-ps = <420>;
		txd2-skew-ps = <360>;
		txd3-skew-ps = <360>;
		txc-skew-ps = <1020>;
		rxc-skew-ps = <960>;
	};
};

&pfe_reserved_slave {
	status = "disabled";
};

&pfe_slave {
	status = "disabled";
};

&pfe {
	status = "okay";
	memory-region = <&pfe_reserved>;
};

&pfe0_if {
	phy-mode = "sgmii";
	phy-handle = <&mdio_a_phy4>;
};

&pfe1_if {
	/* RGMII port occupied by GMAC0 */
	status = "disabled";
	phy-mode = "rgmii";
	phy-handle = <&mdio_c_phy4>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_pfe1_rgmii_c>, <&pinctrl1_pfe1_rgmii_c>;
};

&pfe2_if {
	phy-mode = "rgmii";
	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&pfe0_mdio {
	/* used by USB ULPI */
	status = "disabled";
};

&pfe1_mdio {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_pfe1_mdio_c>, <&pinctrl1_pfe1_mdio_c>;
	/* ARQ107 on S32RGV-VNP-PLAT */
	mdio_c_phy1: ethernet-phy@1 {
		compatible = "ethernet-phy-ieee802.3-c45";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <1>;
	};
	/* KSZ9031RNX on S32G-VNP-PROC */
	mdio_c_phy4: ethernet-phy@4 {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <4>;
	};
	/* KSZ9031RNX on S32RGV-VNP-PLAT */
	mdio_c_phy5: ethernet-phy@5 {
		#address-cells = <1>;
		#size-cells = <0>;
		status = "disabled"; /* blocked by USB by default */
		reg = <5>;
	};
};

&pfe2_mdio {
	/* KSZ9031RNX over SJA1105Q P1 */
	mdio_a_phy2: ethernet-phy@2 {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <2>;
	};
	/* KSZ9031RNX over SJA1105Q P2 */
	mdio_a_phy3: ethernet-phy@3 {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <3>;
	};
	/* ARQ107 on S32RGV-VNP-PROC */
	mdio_a_phy4: ethernet-phy@4 {
		compatible = "ethernet-phy-ieee802.3-c45";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <4>;
	};
	/* TJA1102 over SJA1105Q P3 */
	mdio_a_phy6: ethernet-phy@6 {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <6>;
		max-speed = <100>;
	};
	/* TJA1102 over SJA1105Q P4 */
	mdio_a_phy7: ethernet-phy@7 {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <7>;
		max-speed = <100>;
	};
};

&fxosc {
	clock-frequency = <40000000>;
};

&serdes_ext {
	clock-frequency = <125000000>;
};

&generic_timer {

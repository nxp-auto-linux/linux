/dts-v1/;
#include "fsl-s32g398a.dtsi"
/ {
	model = "Freescale S32G398A";
	compatible = "fsl,s32g398a-emu", "fsl,s32g398a",
				 "arm,vexpress,v2p-aarch64", "arm,vexpress";

	chosen {
		stdout-path = "serial1:115200n8";
	};
};

&fxosc {
	clock-frequency = <40000000>;
};

&generic_timer {
	clock-frequency = <5000000>;
};

&usdhc0 {
	status = "okay";
};

&edma0 {
	status = "okay";
};

&edma1 {
	status = "okay";
};

&pfe_reserved  {
	status = "disabled";
};

&pfe_reserved_slave  {
	status = "disabled";
};

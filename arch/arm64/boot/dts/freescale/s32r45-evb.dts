/dts-v1/;
#include "s32r45.dtsi"

/ {
	model = "NXP S32R458-EVB";

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&generic_timer {
	clock-frequency = <5000000>;
};

&usdhc0 {
	no-1-8-v;
	status = "okay";
};
